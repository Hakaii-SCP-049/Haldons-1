		<passive_effect name="HealthMax" operation="base_set" value="200"/>
	</effect_group>
</entity_class> 


<entity_class name="Prophetmuhammad" extends="zombieUMATemplateMale" >
	<property name="Tags" value="entity,zombie,walker"/>
    <property name="Archetype" value="Muhammed"/>
	<property name="WalkType" value="3"/>
	<property name="MoveSpeedAggro" value=".5, 3.35"/>

	<property name="HandItem" value="ammoProjectileterrorist"/>
	<property name="ExperienceGain" value="5000"/>
	<property name="LootDropProb" value="0.99"/> <!-- Feral Zombie. -->
	<property name="LootDropEntityClass" value="EntityLootContainerBandit"/>
	<property name="TimeStayAfterDeath" value="300"/>
	<drop event="Harvest" name="foodRawMeat" count="0" tool_category="Butcher"/> <!-- this triggers the special harvesting animation like on the knife -->
	<drop event="Harvest" name="resourceOilShale" tag="butcherHarvest" count="100"/>	
	<property name="Explosion.ParticleIndex" value="8"/>
	<property name="Explosion.RadiusBlocks" value="5"/>
	<property name="Explosion.RadiusEntities" value="6"/>
	<property name="Explosion.BlockDamage" value="2000"/>
	<property name="Explosion.EntityDamage" value="150"/>
	<property name="Explosion.DamageBonus.earth" value="0"/>
	<effect_group name="Base Effects">
		<passive_effect name="HealthMax" operation="base_set" value="100"/>
		<passive_effect name="HealthMax" operation="perc_set" value="1"/> <!-- Zombie HP scale -->
		<triggered_effect trigger="onSelfFirstSpawn" action="ModifyCVar" target="self" cvar="EntityBoss" operation="set" value="1"/>
	</effect_group>	

</entity_class> 


