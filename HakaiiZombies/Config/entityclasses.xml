	<property name="LootListOnDeath" value="15"/>
	<property name="CorpseBlock" value="air"/><property name="CorpseBlockChance" value="0"/>
</entity_class>

<entity_class name="zombieUMATemplateFemale" extends="zombieUMATemplateMale">
