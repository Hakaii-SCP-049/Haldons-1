<entity_class name="zombieBob" extends="zombieUMATemplateMale" >
	<property name="Tags" value="entity,zombie,walker"/>
    <property name="Archetype" value="ZombieBob"/>
	<property name="WalkType" value="3"/>
	<property name="HandItem" value="meleeHandZombieStripper"/>
	<property name="MaxHealth" value="200"/>
	<property name="ExperienceGain" value="5000"/>
	<property name="LootDropProb" value="0.25"/> <!-- Feral Zombie. -->
	<property name="LootDropEntityClass" value="EntityLootContainerBandit"/>
	<effect_group name="Base Effects">
		<passive_effect name="HealthMax" operation="base_set" value="200"/>
	</effect_group>
</entity_class> 


<entity_class name="muhammad" extends="zombieUMATemplateMale" >
	<property name="Tags" value="entity,zombie,walker"/>
    <property name="Archetype" value="Muhammed"/>
	<property name="WalkType" value="3"/>
	<property name="HandItem" value="meleeHandZombieStripper"/>
